{
    "name": "spryker-demo/user-profile-gui",
    "type": "library",
    "description": "UserProfileGui module",
    "license": "proprietary",
    "require": {
        "php": ">=8.2",
        "spryker/kernel": "^3.75.0",
        "spryker/symfony": "^3.11.1",
        "spryker/user-extension": "^1.1.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "^0.17.18"
    },
    "autoload": {
        "psr-4": {
            "SprykerDemo\\": "src/SprykerDemo/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerDemoTest\\": "tests/SprykerDemoTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
